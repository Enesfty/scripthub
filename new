_G.autoClick = false
Window:AddToggle({ Title = "Fast Clicker", Description = "Auto Click for Race Clicker.", Tab = OtherTab, Callback = function(v) _G.autoClick = v while _G.autoClick do game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("ClickService"):WaitForChild("RF"):WaitForChild("Click"):InvokeServer() task.wait() end end })
